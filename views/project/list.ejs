<div class="page-header">
  <h1> Projects Show </h1>
</div>

<div class="row">
  <div class="col-sm-12">

    <% for (var project, i=0; project = projects[i]; i++) { %>
      <% if (i%3==0) { %>
        </div><div class="row">
      <% } %>
      <div class="col-sm-4">
        <div class="panel panel-default">
          <div class="panel-heading">
              <h3 class="panel-title"><%= project.name || 'Unnamed Project' %></h3>
          </div>
          <div class="panel-body">
            <iframe src="<%= project.url %>" style="width:100%; height: 400px;" border="0" frameborder="no"></iframe>
          </div>
          <div class="panel-footer">
            <a class="pull-right" href="<%=project.url%>" target="_blank">View</a>
            <h5 class="panel-title"><%= project.team || 'Unknown Team' %></h5>
          </div>
        </div>
      </div>
    <% } %>

  </div>
</div>
