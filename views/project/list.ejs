<div class="page-header">
  <h1> Projects Show </h1>
</div>

<div class="row">
  <div class="col-sm-12">

    <% for (var project, i=0; project = projects[i]; i++) { %>
    <div class="panel panel-default">
      <div class="panel-heading"><%= project.team %></div>
      <div class="panel-body">
        <iframe src="<%= project.url %>" style="width:100%; height: 600px;" border="0" frameborder="no"></iframe>
      </div>
    </div>
    <% } %>

  </div>
</div>
